<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>au.com.ferocia.boomstick</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/boomstick/refresh</string>
    </array>

    <key>StartInterval</key>
    <integer>60</integer>

    <key>UserName</key>
    <string>admin</string>

    <key>RunAtLoad</key>
    <true />

    <key>WorkingDirectory</key>
    <string>/usr/local/boomstick</string>

    <key>StandardErrorPath</key>
    <string>/usr/local/boomstick/console.log</string>

    <key>StandardOutPath</key>
    <string>/usr/local/boomstick/console.log</string>

    <key>Debug</key>
    <true/>
  </dict>
</plist>
